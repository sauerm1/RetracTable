{
  "name": "retractable",
  "version": "1.15.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/sauerm1/RetracTable"
  },
  "author": "Mark Sauer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sauerm1/retractable/issues"
  },
  "keywords": [
    "retractable",
    "react",
    "table",
    "datagrid",
    "json"
  ],
  "main": "index.js",
  "files": [
    "dist"
  ],
  "babel": {
    "presets": [
      "@babel/preset-react",
      "minify"
    ]
  },
  "scripts": {
    "dev": "react-scripts start",
    "test": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "build": "rm -rf dist && mkdir dist && babel src/component -d dist --copy-files",
    "format": "prettier --write '**/*.js'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/preset-react": "^7.13.13",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "babel-preset-minify": "^0.5.1",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3"
  }
}
